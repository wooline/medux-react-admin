{
  "name": "medux-react-admin",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "stylelint": "stylelint \"./src/**/*.less\"",
    "eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "types-check": "tsc",
    "lint": "npm run eslint && npm run stylelint && npm run types-check",
    "start": "npm run types-check && cross-env NODE_ENV=development SITE=local node build/start.js",
    "build-local": "npm run types-check && cross-env NODE_ENV=production SITE=local node build/build.js",
    "build-prod": "npm run types-check && cross-env NODE_ENV=production SITE=prod node build/build.js",
    "analyzer": "cross-env NODE_ENV=production SITE=analyzer node build/build.js",
    "push": "scp -r dist/prod/* root@127.0.0.1:/var/www/medux-react-admin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": "eslint",
    "src/**/*.less": "stylelint"
  },
  "author": {
    "name": "wooline",
    "email": "wooline@qq.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=9.0.0"
  },
  "browserslist": [
    "chrome >= 70"
  ],
  "baseConf": {
    "siteName": "@medux",
    "clientPublicPath": "/",
    "server": "http://localhost:7445",
    "mock": true,
    "proxy": {
      "/api/**": {
        "target": "http://192.168.0.232:8000",
        "pathRewrite": {
          "^/api": ""
        },
        "xfwd": true,
        "secure": false,
        "changeOrigin": true,
        "timeout": 3000,
        "proxyTimeout": 3000
      }
    }
  },
  "vendors": {
    "base": [
      "@babel",
      "react",
      "react-dom",
      "@medux",
      "lodash",
      "process",
      "warning",
      "axios",
      "moment",
      "dayjs",
      "performance-now",
      "shallowequal"
    ],
    "ui": [
      "antd",
      "@ant-design",
      "async-validator",
      "resize-observer-polyfill",
      "add-dom-event-listener",
      "dom-align",
      "react-lifecycles-compat",
      "scroll-into-view-if-needed",
      "compute-scroll-into-view",
      "raf",
      "mini-store",
      "rc-[\\w-]+"
    ]
  },
  "dependencies": {
    "@medux/react-web-router": "~0.2.39",
    "@types/lodash": "~4.14.149",
    "antd": "~4.1.1",
    "lodash": "~4.17.15",
    "path-to-regexp": "~6.1.0",
    "react": "~16.13.0",
    "react-dom": "~16.13.0"
  },
  "devDependencies": {
    "@medux/dev-pkg": "~0.2.38",
    "@medux/dev-utils": "~0.2.39",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.2.0",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "antd-dayjs-webpack-plugin": "1.0.0",
    "babel-plugin-import": "~1.13.0",
    "less": "~3.10.1",
    "less-loader": "~5.0.0",
    "postcss-less": "~3.1.4",
    "raw-loader": "^4.0.0",
    "react-markdown": "^4.3.1",
    "react-refresh": "~0.8.1",
    "sass-resources-loader": "~2.0.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "typescript": "~3.8.3"
  }
}
